#include <stdio.h>

void print_ar(int ar[3][5]);

main()
{
	int i;
	int ar1[3][5];
	int ar2[3][5]={0};
	int ar3[3][5]={0,1};
	int ar4[3][5]={0,1,2};
	int ar5[3][5]={{0,1,2}, {3}};
	int ar6[3][5]={{1}, {2}, {3}};
	int ar7[3][5]={{1,2}, {3}, {4, 5, 6}};
	
	print_ar(ar1);
	print_ar(ar2);
	print_ar(ar3);
	print_ar(ar4);
	print_ar(ar5);
	print_ar(ar6);
	print_ar(ar7);
}

void print_ar(int ar[3][5])
{
	int k, n;
	for(k=0;k<3;k++)
	{
		for(n=0;n<5;n++)
		printf("%d\n", ar[k][n]);
	}
	printf("\n");
}
