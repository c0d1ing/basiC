#include <stdio.h>

void print_ar(int ar[5]);

main()
{
	int i;
	int ar1[5];
	int ar2[5]={0};
	int ar3[5]={0,1};
	int ar4[5]={0,1,2};
	int ar5[5]={0,1,2,3};
	int ar6[5]={1};
	int ar7[5]={1,2,3,4};
	int ar8[5]={1,2,3,4,5};

	print_ar(ar1);
	print_ar(ar2);
	print_ar(ar3);
	print_ar(ar4);
	print_ar(ar5);
	print_ar(ar6);
	print_ar(ar7);
	print_ar(ar8);
	
}

void print_ar(int ar[5])
{
	int k;
	for(k=0;k<5;k++)
	{
		printf("%d", ar[k]);
	}
	printf("\n");
}
